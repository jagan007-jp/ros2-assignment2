<?xml version="1.0"?>
<robot name="stickbot">
  <link name="base_link">
    <visual>
      <geometry><box size="0.2 0.2 0.05"/></geometry>
      <origin xyz="0 0 0.025"/>
    </visual>
  </link>

  <link name="link1">
    <visual>
      <geometry><cylinder length="0.6" radius="0.02"/></geometry>
      <origin xyz="0 0 0.3" rpy="1.5708 0 0"/>
    </visual>
  </link>

  <joint name="joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="0.1 0 0.05"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1.0"/>
  </joint>

  <link name="link2">
    <visual>
      <geometry><cylinder length="0.4" radius="0.02"/></geometry>
      <origin xyz="0 0 0.2" rpy="1.5708 0 0"/>
    </visual>
  </link>

  <joint name="joint2" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0 0 0.6"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1.0"/>
  </joint>
</robot>
